{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"dynamicImport": true,
			"decorators": false
		},
		"transform": {
			"react": {
				"pragma": "h",
				"pragmaFrag": "Fragment",
				"throwIfNamespace": true,
				"development": false,
				"useBuiltins": false
			}
		},
		"target": "es2020",
		"loose": false,
		"externalHelpers": false,
		"keepClassNames": true
	}
}
